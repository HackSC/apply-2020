{
  "name": "odyssey",
  "version": "1.0.0",
  "description": "A hackathon management platform",
  "main": "index.js",
  "scripts": {
    "build": "cd client && yarn install && next build",
    "server:install": "yarn --cwd server install",
    "server:dev": "yarn --cwd server dev",
    "server:build": "yarn --cwd server build",
    "server:start": "yarn --cwd server start",
    "server:test": "yarn --cwd server test",
    "client:install": "yarn --cwd client install",
    "client:dev": "yarn --cwd client dev",
    "client:build": "yarn --cwd client build",
    "client:start": "yarn --cwd client start",
    "client:test": "yarn --cwd client test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HackSC/odyssey.git"
  },
  "author": "HackSC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HackSC/odyssey/issues"
  },
  "homepage": "https://github.com/HackSC/odyssey#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "mysql2": "^1.7.0",
    "next": "^9.1.1",
    "next-with-typescript": "^1.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "sequelize": "^5.19.8"
  }
}
